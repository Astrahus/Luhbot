extends layout

block body
  section(class="container")
  #{oi}
    
  div(class="fixed-action-btn", style="bottom:45px: right: 24px")
    a(class="btn-floating btn-large red")
      i(class="large material-icons waves-effect waves-light") mode_edit
    ul
      li
        a(class="btn-floating red")
          i(class="material-icons waves-effect waves-light tooltipped", data-position="left", data-delay="50", data-tooltip="I am tooltip") insert_chart
      li
        a(class="btn-floating yellow darken-1")
          i(class="material-icons waves-effect waves-light tooltipped", data-position="left", data-delay="50", data-tooltip="I am tooltip") format_quote
      li
        a(class="btn-floating green")
          i(class="material-icons waves-effect waves-light tooltipped", data-position="left", data-delay="50", data-tooltip="I am tooltip") publish
      li
        a(class="btn-floating blue")
          i(class="material-icons waves-effect waves-light tooltipped", data-position="left", data-delay="50", data-tooltip="I am tooltip") attach_file
